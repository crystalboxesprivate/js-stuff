{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [


    {
      "type": "node",
      "request": "launch",
      "name": "Launch node-sql-tutorial",
      "cwd": "${workspaceFolder}\\node-sql-tutorial",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\node-sql-tutorial\\src\\index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch postgres tutorial",
      "cwd": "${workspaceFolder}\\node-api-postgres",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\node-api-postgres\\src\\index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Fullstack",
      "cwd": "${workspaceFolder}\\fullstack-app\\backend",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\fullstack-app\\backend\\server.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch graphql",
      "cwd": "${workspaceFolder}\\full-stackapp\\server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\full-stackapp\\server\\src\\index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Server side rendering",
      "cwd": "${workspaceFolder}\\server-side-rendering",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\server-side-rendering\\src\\server.js",
      "runtimeExecutable": "${workspaceFolder}/server-side-rendering/node_modules/.bin/babel-node",
      "runtimeArgs": ["--nolazy"]
    }
  ]
}